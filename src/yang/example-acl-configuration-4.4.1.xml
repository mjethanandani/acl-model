<access-lists xmlns="urn:ietf:params:xml:ns:yang:ietf-access-control-list">
  <acl>
    <name>sample-ipv4-acl</name>
    <type>ipv4-acl-type</type>
    <aces>
      <ace>
        <name>rule1</name>
        <matches>
          <tcp>
            <source-port>
              <lower-port>16384</lower-port>
              <upper-port>16387</upper-port>
            </source-port>
          </tcp>
        </matches>
        <actions>
          <forwarding>drop</forwarding>
        </actions>
      </ace>
    </aces>
  </acl>
</access-lists>
